        <activity
            android:name=".Manage.ManageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Mission.MissionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Manage.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Manage.ManageInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Record.RecordMatchActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Record.RecordMeasureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Manage.ManageStudentInfoActivity"
            android:screenOrientation="portrait" />
        <activity
